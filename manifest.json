{
  "manifest_version": 3,
  "name": "Check Professor",
  "version": "1.0",
  "description": "Check information about professors by highlighting their names.",
  "permissions": [
    "contextMenus",
    "activeTab",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}
